{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "allowJs": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": false,
    "lib": ["dom"]
  }
}
